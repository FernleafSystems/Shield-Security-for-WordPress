
<h6 class="card-title">
	{{ strings.title }}
	<span class="badge badge-info">{{ vars.total_logs }}</span>
</h6>
{% if vars.total_logs <1 %}
	<div class="alert-info alert">{{ strings.no_logs }}</div>
{% else %}
	<div class="overflow_container">
		<table class="table">
			<thead class="thead-light">
			<tr>
				<th scope="col">{{ strings.username }}</th>
				<th scope="col">{{ strings.event }}</th>
				<th scope="col">{{ strings.created_at }}</th>
			</tr>
			</thead>
			<tbody>
			{% for session in vars.logs %}
				<tr>
				<td>
					<span>{{ session.wp_username }}</span>
				</td>
				<td class="w-50">{{ session.event|raw }}</td>
				<td>{{ session.created_at|raw }}</td>
			</tr>
			{% endfor %}
			</tbody>
		</table>
	</div>
{% endif %}