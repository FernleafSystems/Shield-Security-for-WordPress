msgstr "Θέμα εμφάνισης"

msgid "Theme '{{theme}}' was activated."
msgstr "Το θέμα '{{theme}}' ενεργοποιήθηκε."

msgid "Theme '{{theme}}' was downgraded from version {{from}} to version {{to}}."
msgstr "Το θέμα '{{theme}}' υποβαθμίστηκε από την έκδοση {{from}} στην έκδοση {{to}}."

msgid "Theme '{{theme}}' was installed."
msgstr "Το θέμα '{{theme}}' εγκαταστάθηκε."

msgid "Theme '{{theme}}' was uninstalled (v{{version}})."
msgstr "Το θέμα '{{theme}}' απεγκαταστάθηκε (v{{version}})."

msgid "Theme '{{theme}}' was upgraded from version {{from}} to version {{to}}."
msgstr "Το θέμα '{{theme}}' αναβαθμίστηκε από την έκδοση {{from}} στην έκδοση {{to}}."

msgid "Theme Activated"
msgstr "Ενεργοποίηση Θέματος Εμφάνισης"

msgid "Theme Downgraded"
msgstr "Υποβάθμιση Θέματος Εμφάνισης"

msgid "Theme File Edited"
msgstr "Αρχείο Θέματος Τροποποιήθηκε"

msgid "Theme Files"
msgstr "Αρχεία Θέματος"

