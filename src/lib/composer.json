    }
  },
  "require":           {
    "php":                                   ">=7.2",
    "dolondro/google-authenticator":         "^2.2",
    "fernleafsystems/wordpress-plugin-core": "^2.0",
    "fernleafsystems/zxcvbn-php":            "^1.0",
    "nullpunkt/lz-string-php":               "^1.2.0",
    "symfony/polyfill-iconv":                "^1.18",
    "ext-json":                              "*",
    "ext-openssl":                           "*"
  },
  "require-dev":       {
  },
  "minimum-stability": "dev",
  "prefer-stable":     true,
  "autoload":          {
    "psr-4":    {
      "FernleafSystems\\Wordpress\\Plugin\\Shield\\": "src"
    },
    "classmap": [
      "../processors/",
      "../lib/custom/"
    ],
    "files":    [
      "./functions/functions.php",
      "./src/Functions/functions.php"
