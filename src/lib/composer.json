{
  "name":              "apto-shield/requirements",
  "description":       "shield requirement",
  "authors":           [
    {
      "name":     "Paul Goodchild",
      "email":    "paul@paulgoodchild.me",
      "homepage": "https://paulgoodchild.me"
    }
  ],
  "license":           "MIT",
  "keywords":          [
  ],
