    }
  },
  "require":           {
    "php":                                   ">=7.2",
    "dolondro/google-authenticator":         "^2.2",
    "fernleafsystems/wordpress-plugin-core": "2.4.5",
    "fernleafsystems/zxcvbn-php":            "^1.1",
    "symfony/polyfill-iconv":                "^1.27",
    "nyholm/psr7":                           "^1.8",
    "nyholm/psr7-server":                    "^1.0",
    "web-auth/webauthn-lib":                 "^3.3",
    "ext-json":                              "*",
    "ext-openssl":                           "*"
  },
  "minimum-stability": "stable",
  "prefer-stable":     true,
  "autoload":          {
    "psr-4":    {
      "FernleafSystems\\Wordpress\\Plugin\\Shield\\": "src"
    },
    "classmap": [
    ],
    "files":    [
      "./functions/functions.php",
      "./src/Functions/functions.php"
    ]
  }
